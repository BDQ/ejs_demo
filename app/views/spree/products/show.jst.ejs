<div id="content" class="columns omega sixteen" data-hook>
  <% var product = new Spree.Models.Product(JSON.parse(get('product_json'))) %>
  <%= JST['store/templates/products/show']({ product: product }) %>
</div>

<script type="text/javascript">
  Spree.Data._preload.products = <%= get('products_json') %>;
  Spree.Data._preload.taxonomies = <%= get('taxonomies_json') %>;
  Spree.rendered_page = '<%= full_path %>';
</script>
